    <!-- Email -->
    <li><a href="mailto:?Subject=Check out this online web developer school I found!&amp;Body=<%= request.url %>">
    <img width="64" height="64" title="" alt="" src="data:image/png;base64," />
    </a></li>
 
    <!-- Twitter -->
    <li><a href="https://twitter.com/share?url=<%= request.url %>" target="_blank">
    <img width="64" height="64" title="" alt="" src="data:image/png;base64," />
